{
  "version": 2,
  "routes": [
    {
      "src": "/api/slack-webhook/(.*)",
      "dest": "/api/slack-webhook.js"
    }
  ],
  "functions": {
    "api/slack-webhook.js": {
      "maxDuration": 10
    },
    "api/queue.js": {
      "maxDuration": 10
    },
    "api/delete-message.js": {
      "maxDuration": 10
    },
    "api/webhook.js": {
      "maxDuration": 10
    },
    "api/oauth2/callback/index.js": {
      "maxDuration": 10
    }
  },
  "buildCommand": "echo 'Build command executed'",
  "cleanUrls": true
} 